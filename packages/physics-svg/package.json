    "@vitejs/plugin-react": "^4.4",
    "react": "^19.0",
    "react-dom": "^19.0",
    "gsap": "^3.12",
    "vite": "^6.2",
    "vite-plugin-dts": "^4.5",
    "vitest": "^3.0",
    "typescript": "^5.7"
  }
